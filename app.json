{
  "name": "physiqual.com",
  "scripts": {
    "postdeploy": "bundle exec rake db:setup"
  },
  "env": {
    "FITBIT_CLIENT_ID": {
      "required": true
    },
    "FITBIT_CLIENT_SECRET": {
      "required": true
    },
    "GOOGLE_CLIENT_ID": {
      "required": true
    },
    "GOOGLE_CLIENT_SECRET": {
      "required": true
    },
    "HOST_PROTOCOL": {
      "required": true
    },
    "HOST_URL": {
      "required": true
    },
    "LANG": {
      "required": true
    },
    "RACK_ENV": {
      "required": true
    },
    "RAILS_ENV": {
      "required": true
    },
    "RAILS_SERVE_STATIC_FILES": {
      "required": true
    },
    "SECRET_KEY_BASE": {
      "required": true
    }
  },
  "addons": [
    "newrelic",
    "papertrail",
    "heroku-postgresql"
  ]
}
